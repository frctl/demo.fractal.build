{
  "name": "@frctl/demo.fractal.build",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "clean": "rimraf dist",
    "start": "concurrently 'fractal start --sync' 'webpack --mode development --watch'",
    "build": "npm run clean && npm run build:webpack && npm run build:fractal",
    "build:webpack": "NODE_ENV=production webpack --mode production",
    "build:fractal": "fractal build"
  },
  "dependencies": {
    "@frctl/fractal": "^1.5.6",
    "@frctl/mandelbrot": "^1.9.0",
    "@frctl/nunjucks": "^2.0.10"
  },
  "devDependencies": {
    "autoprefixer": "^10.2.5",
    "concurrently": "^6.0.2",
    "css-loader": "^5.2.4",
    "mini-css-extract-plugin": "^1.5.1",
    "postcss": "^8.2.13",
    "postcss-loader": "^5.2.0",
    "rimraf": "^3.0.2",
    "tailwindcss": "^2.1.2",
    "webpack": "^5.36.1",
    "webpack-cli": "^4.6.0"
  }
}
